{
	"name": "rncom_nextjs",
	"description": "RNCOM is a an Ecommerce project designed by a software developer for software developers",
	"version": "1.0.0",
	"scripts": {
		"dev": "next dev -p 3021",
		"build": "next build",
		"start": "next start -p 3021",
		"lint": "stylelint --allow-empty-input \\\"**/*.{css,scss,sass}\\\" && next lint",
		"lint_watch": "onchange -i \"src/**/*.tsx\" \"src/**/*.ts\" -- npm run -s lint",
		"typecheck": "npm run typecheck_cypress ; npm run typecheck_project",
		"typecheck_cypress": "(cd ./cypress && tsc --noEmit | npx loose-ts-check | grep -v errors) ",
		"typecheck_project": "tsc --noEmit",
		"typecheck_watch": "onchange -i \"src/**/*.tsx\" \"src/**/*.ts\" -- npm run -s typecheck",
		"watch": "npm run -s lint_watch & npm run -s typecheck_watch",
		"codegen": "graphql-codegen --config codegen.ts",
		"build_translation": "npx ts-node -O '{\"module\": \"commonjs\"}' -e \"import { asJson } from './src/translations'; import { writeFileSync } from 'fs'; writeFileSync(\\\"./messages/en.json\\\", asJson())\"",
		"test": "npx cypress run --spec \"cypress/e2e/all_e2e_tests.cy.ts\"",
		"cypress_dev": "npx cypress open --e2e --browser electron",
		"cypress_dev_parallel": "cypress-parallel --script cypress_dev_parallel_thread --threads 3 --specsDir \"./cypress/e2e\"",
		"cypress_dev_parallel_thread": "RUNNER=cypress-parallel cypress run",
		"preinstall": "npm -s --prefix .. run copy_docs"
	},
